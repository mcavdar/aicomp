            </tr>
            </thead>
            <tbody>


            <tr class="{% cycle row1,row2 %}">
                <th> Exact Span</a></th>
                <th>{{ iaaStats.exactSpan.iaaPercent}}</a></th>
                <th>{{ iaaStats.exactSpan.matches}}</a></th>
                <th>{{ iaaStats.exactSpan.nonMatches}}</a></th>

