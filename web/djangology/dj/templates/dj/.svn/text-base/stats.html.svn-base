            </tbody>
        </table>


        <!--<p class="paginator">-->

        <!--{{ doc_list|length }} abstracts-->

        <!--</p>-->

    </div>


</div>


<br class="clear"/>

<!------------ >

<h1>2-way IAA Results - Classless matches</h1>


<div id="content-main">


    <div class="module changelist">


        <table cellspacing="0">
            <thead>
            <tr>
                <th>
                    Type
                </th>
