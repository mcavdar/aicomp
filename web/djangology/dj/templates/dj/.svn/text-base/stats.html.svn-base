    <div class="module changelist">


        <table cellspacing="0">
            <thead>
            <tr>

                <th>
                    Gold Standard Set
                </th>
                <th>
                    Compared Set
                </th>
                <th>
                    Type
                </th>
                <th>
                    True Positives
                </th>
                <th>
                    False positives
                </th>
                <th>
                    False negarives

                </th>
                <th>
                    Precision
                </th>

                <th>
                    Recall
                </th>
                <th>
                    F score
                </th>


            </tr>
            </thead>
            <tbody>


            {% for k,v in iaaStats.classExactSpan.items%}
